<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item
        android:right="@dimen/border_half"
        android:top="@dimen/border">
        <shape android:shape="rectangle">
            <stroke
                android:width="@dimen/border"
                android:color="@color/border_color" />
            <corners
                android:bottomLeftRadius="@dimen/radius"
                android:bottomRightRadius="@dimen/no_radius"
                android:topLeftRadius="@dimen/no_radius"
                android:topRightRadius="@dimen/no_radius" />
        </shape>
    </item>
</layer-list>


